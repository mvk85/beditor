{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "key-spacing": 0,
    "react/prefer-stateless-function": 0,
    "no-return-assign": 0,
    "no-param-reassign": 0,
    "no-sequences": 0,
    "global-require": 0,
    "import/no-unresolved": 0,
    "arrow-body-style": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "prefer-template": 0,
    "consistent-return": 0,
    "react/jsx-no-bind": 0,
    "new-cap": 0
  }
}
